{{#if categoryItems}}

<article class="fmj-stickers">
  <hr class="cut-here">


  <div class="m-t-2 m-b-3 text-xs-left alert alert-warning">
    <p>Merci d'imprimer cette page sur papier blanc.</p>
    <p>
      <i class="fa fa-warning"></i>
      Les étiquettes ci-dessous seront <strong>indispensables</strong>
      lors de l'enregistrement des pièces.
    </p>
  </div>

  {{#each categoryItems}}
  <div class="card fmj-sticker" >
    <div class="card-header">
      <div class="row">
        <div class="fmj-code col-xs-2">
          {{ @key }}
        </div>
        <div class="fmj-category col-xs-10 text-xs-right">
          {{ category_name @key }}<br>
          <small>{{ category_group @key }}</small>
        </div>
      </div>
    </div>

    <div class="card-block">
      {{#each this}}
      {{#if true}}
      <dl>
        <dt>{{row_number @index}}</dt>
        <dd>{{this}}</dd> 
      </dl>
      {{/if}}
      {{/each}}
    </div>

    <div class="card-footer">
      N°&nbsp;: <var class="fmj-id">{{ ../userInfo.accessKey }}</var>
    </div>
  </div>
  {{/each}}

</article>
{{/if}}
